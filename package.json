{
  "name": "tuitter",
  "productName": "Tui",
  "version": "0.0.1",
  "description": "Twitter client based on mobile.twitter.com in menu bar",
  "main": "main/index.js",
  "bin": {
    "tuitter": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc --pretty -p .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tslint": "tslint $(git ls-files | grep -E \"\\.tsx?$\")",
    "stylelint": "stylelint renderer/style.css webview/style.css",
    "lint": "npm-run-all --parallel tslint stylelint",
    "watch": "guard --watchdir main renderer typings webview",
    "debug": "NODE_ENV=development electron .",
    "start": "NODE_ENV=production electron .",
    "preversion": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhysd/Tui.git"
  },
  "keywords": [
    "Twitter",
    "client",
    "menubar",
    "electron",
    "desktop",
    "application"
  ],
  "author": "rhysd <lin90162@yahoo.co.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhysd/Tui/issues"
  },
  "homepage": "https://github.com/rhysd/Tui#readme",
  "devDependencies": {
    "@types/electron": "^1.4.31",
    "@types/loglevel": "^1.4.29",
    "@types/menubar": "^5.1.3",
    "@types/mousetrap": "^1.5.33",
    "@types/node": "^6.0.58",
    "npm-run-all": "^4.0.0",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^15.0.1",
    "tslint": "^4.3.1",
    "typescript": "^2.1.4"
  },
  "dependencies": {
    "electron": "^1.4.13",
    "loglevel": "^1.4.1",
    "menubar": "github:rhysd/menubar#ensure-to-launch-app",
    "mousetrap": "^1.6.0"
  }
}
